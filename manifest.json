{
    "manifest_version": 3,
    "name": "Tabs Exporter",
    "description": "Export windows and tabs to a file",
    "author": "CGC",
    "version": "1.0.0",
    "permissions": ["tabs", "windows", "downloads"],
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "48": "assets/icon-48x48.png",
        "128": "assets/icon-128x128.png"
    }
}